{
    "author": "DataFoundry Team",
    "description": "A guide about DataFoundry",
    "title": "DataFoundry手册",
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "anchors",
        "multipart",
        "-toc-ng",
        "page-footer-ex",
        "timeline",
        "katex"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "toc": {
            "addClass": true,
            "className": "toc"
        },
        "page-footer-ex": {
            "copyright": "By <em>DataFoundry Team</em>",
            "markdown": false,
            "update_label": "<i>updated</i>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
